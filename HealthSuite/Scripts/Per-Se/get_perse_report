
echo
echo "-----------------------------------"
date
echo "-----------------------------------"
echo

PGPFILE=phy169ms.zip.pgp

ftp depot.medaphis.com <<ENDOFHEREDOC
cd ../outgoing
get $PGPFILE
bye
ENDOFHEREDOC

if [ ! -f $PGPFILE ]; then
	echo "No new report from Perse."
	exit
fi

process_report

if [ $? != 0 ]; then
	echo
	echo "process_report returns non-zero status code.  Aborted."
	exit
fi

ftp depot.medaphis.com <<ENDOFHEREDOC
cd ../outgoing
delete $PGPFILE
bye
ENDOFHEREDOC
	