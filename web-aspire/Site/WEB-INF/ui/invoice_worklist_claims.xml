			<field.text caption="User ID" read-only="yes" name="user_id"/>
			<field.memo caption="Comments" name="comments" size="24"/>

		</dialog>

		<dialog name="transfer_claims_wl_invoice" heading="transfer_claims_wl_invoice">
			<field.text caption="Date" read-only="yes" name="date"/>
			<field.text caption="Invoice ID" read-only="yes" name="invoice_id"/>
			<field.text caption="Transfer From" read-only="yes" name="user_id"/>
			<field.person.id name="responsible_id" caption="Transfer To" required="yes" style="text"/>
