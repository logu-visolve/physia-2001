			<field.text caption="User ID" read-only="yes" name="user_id"/>
			<field.scheduling.date name="reck_date" caption="Reck Date" required="yes"/>
			<!-- Total Field contents:
				_spacerWidth => 0
				addPopup => 
				caption => Reck Date
				defaultValue => 11/14/2001
				findPopup => 
				findPopupAppendValue => 
				flags => 3600
				formatValue => CODE(0x8617e68)
				hint => 
				hints => 
				lengthInvalidMsg => Reck Date is too long (can be at most 10 characters).
				maxLength => 10
				message => 
				name => reck_date
				onBlurJS => validateChange_Date(event)
